# FaceCheck.id API Configuration
# Copy this file to .env and replace with your actual API token

# Your FaceCheck.id API Token
# Get your token from: https://facecheck.id/Face-Search/API
FACECHECK_API_TOKEN=your_api_token_here

# SERP API Configuration
# Get your API key from: https://serpapi.com/
SERPAPI_KEY=your_serpapi_key_here

# LLM API Configuration
# OpenAI API key (optional if using Cerebras)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Cerebras API key (recommended for faster inference)
# Get your API key from: https://cloud.cerebras.ai/
CEREBRAS_KEY=your_cerebras_api_key_here

# LLM Provider Selection (cerebras, openai)
# Default: cerebras (if key available), fallback to openai
LLM_PROVIDER=cerebras

# LLM Model Selection (optional - use defaults if not specified)
# Cerebras models: gpt-oss-120b (recommended), llama3.1-8b, llama3.1-70b, llama3.1-405b
# gpt-oss-120b: $0.35/M input tokens, $0.75/M output tokens, up to 3,000 tokens/sec
CEREBRAS_MODEL=gpt-oss-120b

# OpenAI models: gpt-3.5-turbo, gpt-4, gpt-4o, gpt-4-turbo
OPENAI_MODEL=gpt-4o

# Testing mode (true/false)
# Set to false when you want to use real credits for face search
# true = demo mode (no credits used), false = production mode (uses credits)
TESTING_MODE=true
